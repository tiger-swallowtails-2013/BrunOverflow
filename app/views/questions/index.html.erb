<%= link_to 'Sign Up', new_user_path %>
<%= link_to 'Sign In', login_path %>
<% if current_user %>
  <p>Welcome <%=current_user.email%></p>
<% end %>
<a href="<%= new_question_path %>">Ask a Question</a>
<div id="questions">
  <ul>
    <% @questions.each do |question| %>
      <li class="question"><%= link_to question.title, question %></li>
    <% end %>
  </ul>
</div>